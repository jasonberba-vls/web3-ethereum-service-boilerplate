{
  "name": "api-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:details": "jest --verbose",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clear_jest": "jest --clearCache",
    "test:single": "jest -t 'EncryptionService'",
    "test:specific": "jest -t 'ProcessWithdrawalUseCase v1.7'",
    "proto:clean": "rm -rf src/proto/generated/*",
    "proto:generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --proto_path=./src/proto/sourcefiles --ts_proto_out=./src/proto/generated ./src/proto/sourcefiles/*.proto --ts_proto_opt=nestJs=true",
    "proto:start": "npm run proto:clean && npm run proto:generate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.358.0",
    "@aws-sdk/s3-request-presigner": "^3.358.0",
    "@grpc/grpc-js": "^1.11.2",
    "@grpc/proto-loader": "^0.7.13",
    "@grpc/reflection": "^1.0.4",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/platform-express": "^10.3.9",
    "amqplib": "^0.10.3",
    "avsc": "^5.7.7",
    "axios": "^1.7.0",
    "class-validator": "^0.14.0",
    "cls-rtracer": "^2.6.3",
    "compression": "^1.7.4",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "kafkajs": "^2.2.4",
    "md5": "^2.3.0",
    "mysql2": "^3.4.0",
    "nest-winston": "^1.9.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sha.js": "^2.4.11",
    "ts-proto": "^2.2.0",
    "utf8": "^3.0.0",
    "web3": "^4.15.0",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.5.0",
    "jest-junit": "^16.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      ".module.ts",
      "main.ts",
      "index.ts"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "",
          "outputName": "test-results.xml"
        }
      ]
    ]
  }
}
